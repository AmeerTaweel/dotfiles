# Main
snippet main "main function" b
int main(int argc, char* argv[]) {
	${0}
	return 0;
}
endsnippet

# Main Void
snippet mainv "main function with no parameters" b
int main(void) {
	${0}
	return 0;
}
endsnippet

# Include Standard
snippet incs "include standard file" b
#include <${1:stdio}.h>
endsnippet

# Include Custom
snippet incc "include custom file" b
#include "${1:file}.h"
endsnippet
